<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.pavan.kmtabalavidyalaya" >
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@drawable/kmtvsplash"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".Activities.Splash"
            android:label="@string/app_name"
            >

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".Activities.MainActivity"
            android:label="@string/app_name"
            >
        </activity>

        <activity
            android:name=".Activities.ReachOut"
            android:label="@string/app_name"
            >

        </activity>
        <activity
            android:name=".Activities.AboutGuru"
            android:label="@string/app_name"
            >

        </activity>



        <activity
            android:name=".Activities.GuruDetails"
            android:label="@string/app_name"
            >
            <intent-filter>
                <action android:name="android.intent.action.guru" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <activity
            android:name=".Activities.NewsFeed"
            android:label="@string/app_name"
            >

        </activity>


    </application>

</manifest>
